package Array;

import java.util.Scanner;

public class Exercicio_2 {

	public static void main (String args[]) {
		
		Scanner entrada = new Scanner(System.in);
		
		int [] [] a = new int [3] [3];
		int contador=0;
		int contador1=0;
		int contotal;
			
		
		for(int linha=0 ; linha < 3 ; linha ++)
		 for(int coluna=0 ; coluna < 3 ; coluna++) {
			
			System.out.printf("Digite os valores das linhas e colunas [%d] [%d]: ",linha+1, coluna+1);
				a[linha][coluna]=entrada.nextInt();
			
		
		if(linha > 10)
		{
			contador++;
					
		}
		if(coluna > 10)
		{
			contador1++;
		}
		 }
		
		{
			contotal = contador + contador1;
			{
				System.out.println("A quantidade total de números 10 digitados na matriz foi de: "+contotal);
			}
		}
			
		}
					
		}
		
		
		

